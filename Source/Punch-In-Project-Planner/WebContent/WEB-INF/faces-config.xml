<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/sections/employee/timesheets.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>viewTimesheet</from-outcome>
			<to-view-id>/sections/employee/timesheetRows.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>viewProject</from-outcome>
			<to-view-id>/sections/manager/project_details.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>editProject</from-outcome>
			<to-view-id>/sections/manager/project_edit.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>project_wp_edit</from-outcome>
			<to-view-id>/sections/manager/project_wp_edit.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>viewEmployee</from-outcome>
			<to-view-id>/sections/manager/employee_details.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>editEmployee</from-outcome>
			<to-view-id>/sections/manager/employee_edit.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>allEmployee</from-outcome>
			<to-view-id>/sections/manager/employees.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<application>
		<resource-bundle>
			<base-name>ca.bcit.PunchIn.messages</base-name>
			<var>msgs</var>
		</resource-bundle>
		<message-bundle>ca.bcit.PunchIn.messages</message-bundle>
	</application>
</faces-config>
